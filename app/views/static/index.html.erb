<%= link_to("Login", new_staff_session_path) if !staff_signed_in? %>
<%= link_to("Logout", destroy_staff_session_path, method: "DELETE") if staff_signed_in? %>
<% if staff_signed_in? %>
	
<h1>Welcome <%= current_staff.name %></h1>
	<% if can? :update, current_staff %>
